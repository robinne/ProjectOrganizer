{
  "name": "Angular2-App",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"electron main.js\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install && npm run rebuild-sqlite3",
    "rebuild-sqlite3": "cd node_modules/sqlite3 && npm run prepublish && node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/electron-v0.37-win32-x64 && node-gyp rebuild --target=0.37.8 --arch=x64 --target_platform=darwin --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/electron-v0.37-win32-x64"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^2.0.0-rc.1",
    "@angular/core": "^2.0.0-rc.1",
    "@angular2-material/checkbox": "^2.0.0-alpha.4",
    "@angular2-material/core": "^2.0.0-alpha.4",
    "ag-grid": "^4.0.5",
    "ag-grid-ng2": "^4.0.4",
    "angular2": "2.0.0-beta.15",
    "body-parser": "^1.15.0",
    "bootstrap": "^3.3.6",
    "es6-shim": "^0.35.0",
    "express": "^4.13.4",
    "node-pre-gyp": "^0.6.26",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.25",
    "zone.js": "0.6.6"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "electron-prebuilt": "^0.37.0",
    "electron-rebuild": "^1.1.3",
    "lite-server": "^2.2.0",
    "node-gyp": "^3.3.1",
    "typescript": "^1.8.9",
    "typings": "^0.7.12"
  }
}
